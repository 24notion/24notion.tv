@{
    ViewBag.Title = "#24NotionTheCatalyst";
}
<section id="page" class="home">
    @Html.Partial("_PageHeader",
        new ViewDataDictionary
        {
            { "TitleHeader",  @"<h2>" + @ViewBag.Title + "</h2>"},
            //{ "TitleContent", @ViewBag.Title },
            { "TitleImageSrc", Url.Content("~/Content/images/home/_catalyst_a.jpg") }
        })

    <div id="_video-teaser" class="section-block">
        <div class="image-area">
            <video controls preload="auto"
                   controlsList="nodownload"
                   poster="~/Content/images/home/video_lasvegas_arial.jpg"
                   style="width: 100%;">
                <source src="~/Content/videos/home/Teaser_Highlight_HQ_1280_tbr.webm" type="video/mp4" />
