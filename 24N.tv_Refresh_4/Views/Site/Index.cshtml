            </video>
        </div>
        <div class="container-fluid content-area">
            <div class="row">
                <br />
                <div class="col-xs-8 col-sm-6 text-right">
                </div>
                <div class="col-xs-4 col-sm-6 sectionnav text-right" style="margin-right: 15px; z-index: 2;">
                    <div class="sectionnav">
                        <a href="#_redefine" class="nohighlight">
